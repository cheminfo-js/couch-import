    "supertest": "^3.3.0",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-serve": "^2.0.2",
    "whatwg-fetch": "^3.0.0"
  }
}
