{
  "name": "rest-on-couch",
  "version": "1.2.0",
  "description": "Interface to CouchDB that allows the control of permissions on the documents",
  "main": "lib/index.js",
  "files": [
    "assets",
    "bin",
    "lib",
    "views"
  ],
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack",
    "build:server": "npm run compile",
    "compile": "babel src --out-dir lib",
    "eslint": "eslint src bin test",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublish": "npm run compile",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node bin/rest-on-couch-server.js",
    "test": "npm run compile && npm run test-mocha && npm run eslint && sh test/checkOnly.sh",
    "test-cov": "istanbul cover -x '**/design/**' _mocha -- --require should --require ./test/setup",
    "test-mocha": "mocha --timeout 5000 --require should --require ./test/setup --reporter mocha-better-spec-reporter --recursive",
    "watch": "npm run compile -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rest-on-couch.git"
  },
  "keywords": [
    "couch",
    "couchdb"
  ],
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rest-on-couch/issues"
  },
  "homepage": "https://github.com/cheminfo/rest-on-couch#readme",
  "dependencies": {
    "chokidar": "^1.4.2",
    "co": "^4.6.0",
    "co-sleep": "0.0.1",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "escape-string-regexp": "^1.0.4",
    "extend": "^3.0.0",
    "fold-to-ascii": "^2.0.2",
    "fs-extra": "^0.30.0",
    "has-own": "^1.0.0",
    "if-env": "^1.0.0",
    "kcors": "^1.0.1",
    "koa": "^1.1.2",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.9",
    "koa-passport": "^1.3.0",
    "koa-proxy": "^0.6.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^5.3.0",
    "koa-serve": "^0.1.7",
    "koa-session": "^3.1.0",
    "ldapjs": "^1.0.0",
    "lodash": "^4.9.0",
    "md5": "^2.2.1",
    "minimist": "^1.2.0",
    "nano": "^6.1.5",
    "nunjucks": "^2.5.2",
    "object-hash": "^1.1.4",
    "passport-facebook": "^2.0.0",
    "passport-github": "^1.0.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-ldapauth": "^0.6.0",
    "passport-local": "^1.0.0",
    "randomatic": "^1.1.5",
    "raw-body": "^2.1.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "next",
    "request": "^2.74.0",
    "request-promise": "^4.0.2",
    "superagent": "^2.0.0",
    "superagent-promise": "^1.1.0",
    "thenify-all": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-spread": "^6.6.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.1.1",
    "eslint-config-cheminfo": "^1.1.1",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "istanbul": "^0.4.3",
    "mocha": "^3.0.0",
    "mocha-better-spec-reporter": "^3.0.2",
    "should": "^11.1.0",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
