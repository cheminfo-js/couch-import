    "chokidar": "^2.0.4",
    "co-sleep": "0.0.1",
    "commander": "^2.17.1",
    "debug": "^3.1.0",
    "delay": "^3.1.0",
    "escape-string-regexp": "^1.0.5",
    "extend": "^3.0.2",
    "fold-to-ascii": "^4.0.0",
    "fs-extra": "^7.0.0",
    "has-own": "^1.0.0",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "kcors": "^2.2.2",
    "klaw": "^3.0.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-passport": "^4.1.0",
