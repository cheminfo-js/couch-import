{
  "name": "rest-on-couch",
  "version": "5.8.0",
  "description": "Interface to CouchDB that allows the control of permissions on the documents",
  "main": "src/index.js",
  "files": [
    "bin",
    "public",
    "src",
    "views"
  ],
  "bin": {
    "rest-on-couch-import": "./bin/rest-on-couch-import.js"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./test/setup/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/data/",
      "/test/homeDirectories/",
      "/test/utils/",
      "/test/setup/"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "eslint": "eslint src bin test --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier-eslint bin/*.js src/**/*.js test/**/*.js --write",
    "start:dev": "webpack-serve --content public/",
    "start:prod": "NODE_ENV=production npm run build && node bin/rest-on-couch-server.js",
    "start:devserver": "REST_ON_COUCH_HOME_DIR=test/homedir node bin/rest-on-couch-server.js --config=conf/devConfig.js",
    "test": "npm run test-only && npm run eslint",
    "test-coverage": "npm run test-only -- --coverage",
    "test-only": "jest --runInBand",
    "test-travis": "npm run test-coverage && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rest-on-couch.git"
  },
  "keywords": [
    "couch",
    "couchdb"
  ],
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rest-on-couch/issues"
  },
  "homepage": "https://github.com/cheminfo/rest-on-couch#readme",
  "dependencies": {
    "agentkeepalive": "^3.5.1",
    "chokidar": "^2.0.4",
    "commander": "^2.18.0",
    "debug": "^4.0.1",
    "delay": "^4.0.1",
    "extend": "^3.0.2",
    "fold-to-ascii": "^4.0.0",
    "fs-extra": "^7.0.0",
    "has-own": "^1.0.0",
    "kcors": "^2.2.2",
    "klaw": "^3.0.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-hbs": "^1.0.0",
    "koa-passport": "^4.1.1",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.9.0",
    "koa-static": "^5.0.0",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "nano": "^7.1.0",
    "object-hash": "^1.3.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-ldapauth": "^2.1.0",
    "passport-local": "^1.0.0",
    "randomatic": "^3.1.0",
    "raw-body": "^2.3.3",
    "request": "^2.88.0",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0",
    "zenodo": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-config-neptune-react": "^2.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jquery": "^3.3.1",
    "make-promises-safe": "^1.1.0",
    "prettier-eslint-cli": "^4.7.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select2-wrapper": "^1.0.4-beta6",
    "redux": "^4.0.0",
